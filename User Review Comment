def update_test_coverage_report(pr_files, test_files):
    total_relevant_lines = 0
    total_covered_lines = 0

    for file in pr_files:
        total_relevant_lines += len(file.lines)

    for file in test_files:
        for line in file.lines:
            if line.is_covered:
                total_covered_lines += 1

    overall_coverage = (total_covered_lines / total_relevant_lines) * 100

    return {
        "total_relevant_lines": total_relevant_lines,
        "total_covered_lines": total_covered_lines,
        "overall_coverage": overall_coverage,
    }

def check_test_coverage_report(updated_report, original_report):
    assert updated_report["total_relevant_lines"] == original_report["total_relevant_lines"]
    assert updated_report["total_covered_lines"] == original_report["total_covered_lines"]
    assert updated_report["overall_coverage"] == original_report["overall_coverage"]
